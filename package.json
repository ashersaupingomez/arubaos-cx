{
  "name": "arubaos-cx",
  "version": "3.0.4",
  "description": "Superagent utilities for interacting with the ArubaOS-CX REST API",
  "main": "dist/cjs",
  "module": "dist/esm",
  "scripts": {
    "clean": "rm -rf dist",
    "build:docs": "documentation readme src --section=API --parse-extension=ts",
    "build:cjs": "tsc --outDir dist/cjs",
    "build:esm": "tsc --outDir dist/esm --module es2020",
    "build": "npm run build:cjs && npm run build:esm",
    "prepare": "npm run clean && npm run build",
    "test": "npm run prepare && node dist/cjs/useClient.test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ashersaupingomez/arubaos-cx.git"
  },
  "author": "Asher Gomez <ashersaupingomez@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ashersaupingomez/arubaos-cx/issues"
  },
  "homepage": "https://github.com/ashersaupingomez/arubaos-cx#readme",
  "dependencies": {
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/baretest": "^2.0.0",
    "@types/node": "^14.14.22",
    "@types/superagent": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "baretest": "^2.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "typescript": "^4.2.2"
  }
}
